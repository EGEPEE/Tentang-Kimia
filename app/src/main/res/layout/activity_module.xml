<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgpattern"
    android:padding="15dp"
    tools:context="egaprasetianti.finalproject.ModuleActivity">

    <TextView
        android:id="@+id/mod"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@color/colorSplashTrans"
        android:gravity="center"
        android:padding="5dp"
        android:text="MODULE"
        android:textColor="@color/pickled"
        android:textSize="25dp"
        android:textStyle="bold" />

        <android.support.v7.widget.RecyclerView
            android:id="@+id/recyclerViewmodule"
            android:layout_below="@+id/mod"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.RecyclerView>

</RelativeLayout>
